<h1>Vacation Homes</h1>

<% @vacation_homes.each do |home| %>
  <h3><a href="/vacation_homes/<%= home.id %>"><%= home.listing_name %></a></h3>
  <p>Location: <%= home.location%></p>
  <p>Created at: <%= home.created_at%></p>
  <a href="/vacation_homes/<%= home.id %>/edit">Edit</a><br>
  <%= button_to 'Delete Vacation Home', "/vacation_homes/#{home.id}", method: :delete %>
<% end %><br>

